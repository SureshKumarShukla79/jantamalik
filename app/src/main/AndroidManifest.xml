<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.filternet.jantamaalik">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme_NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DonateActivityJava.donate"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".VoteJava.VoteMP"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".IssuesJava.IssuesItem"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name="in.filternet.jantamaalik.Contact"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:excludeFromRecents="true"
            android:label="Contact US"
            android:parentActivityName="in.filternet.jantamaalik.MainActivity" />
    </application>

</manifest>